{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "FUNCTIONS_WORKER_RUNTIME": "dotnet"
  },
  "Curator": {
    "WithDryRun": true,  // true => will not perform index DELETE
    "RequestTimeout": 4, // in seconds
    "Entries": [
      {
        "Name": "Docker",
        "Endpoint": "http://127.0.0.1:9200",
        "Indices": [
          {
            "Prefix": "sample-",
            "RetentionDays": 14
          }
        ]
      }
    ]
  }
}